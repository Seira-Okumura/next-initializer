{
  "typescript.validate.enable": false,
  "eslint.validate": [
    "javacript",
    "javacriptreact"
  ],
  "eslint.enable": true,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "files.associations": {
    "*.jsx": "javascriptreact"
  },
  "files.autoGuessEncoding": true
}
